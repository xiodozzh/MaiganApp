<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PulseWaveGraphView">
        <!--线颜色-->
        <attr name="pulse_line_color" format="color"/>
        <!--线宽-->
        <attr name="pulse_line_width" format="dimension"/>
        <!--背景色-->
        <attr name="pulse_bg" format="color"/>
        <!--数组的最大值-->
        <attr name="pulse_scope" format="integer"/>
        <!--一屏中点的个数-->
        <attr name="pulse_point_number" format="integer"/>

    </declare-styleable>
</resources>